<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <properties>
     <sourceforge.project.groupId>166725</sourceforge.project.groupId>
  </properties>
  <modelVersion>4.0.0</modelVersion>
  <groupId>uk.co.flamingpenguin.jewelcli</groupId>
  <artifactId>jewelcli</artifactId>
  <packaging>jar</packaging>
  <version>0.6</version>
  <name>JewelCli</name>
  <url>http://jewelcli.sourceforge.net</url>
  <description>JewelCli uses an annotated interface definition to automatically parse and present command line arguments</description>
  <inceptionYear>2006</inceptionYear>
  <organization>
    <name>flamingpenguin</name>
    <url>http://www.flamingpenguin.co.uk</url>
  </organization>
  <distributionManagement>
    <downloadUrl>http://sourceforge.net/project/showfiles.php?group_id=166725</downloadUrl>
    <site>
      <id>jewelCli.sourceforge</id>
      <url>scp://shell.sourceforge.net/home/groups/j/je/jewelcli/htdocs</url>
    </site>
    <repository>
      <id>jewelCli.sourceforge</id>
      <name>JewelCli Repository at sourceforge</name>
      <url>scp://shell.sourceforge.net/home/groups/j/je/jewelcli/htdocs/m2repo</url>
    </repository>
    <snapshotRepository>
      <id>jewelCli.sourceforge</id>
      <name>JewelCli Repository at sourceforge</name>
      <url>scp://shell.sourceforge.net/home/groups/j/je/jewelcli/htdocs/m2repo-snapshot</url>
    </snapshotRepository>
  </distributionManagement>
  <licenses>
    <license>
      <name>Apache License 2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
    </license>
  </licenses>
  <developers>
    <developer>
      <id>flamingpenguin</id>
      <name>Tim Wood</name>
      <email>flamingpenguin@gmail.com</email>
      <roles>
        <role>Project Manager</role>
        <role>Architect</role>
      </roles>
      <timezone>0</timezone>
    </developer>
  </developers>
  <issueManagement>
    <url>http://sourceforge.net/tracker/?group_id=${sourceforge.project.groupId}</url>
  </issueManagement>
  <scm>
    <connection>scm:svn:https://jewelcli.svn.sourceforge.net/svnroot/jewelcli/JewelCli/tags/release-0.6</connection>
    <developerConnection>scm:svn:https://jewelcli.svn.sourceforge.net/svnroot/jewelcli/JewelCli/tags/release-0.6</developerConnection>
    <url>http://jewelcli.svn.sourceforge.net/viewvc/jewelcli/JewelCli/tags/release-0.6</url>
  </scm>
  <mailingLists>
    <mailingList>
      <name>JewelCli users' mailing list</name>
      <subscribe>https://lists.sourceforge.net/lists/listinfo/jewelcli-users</subscribe>
      <unsubscribe>https://lists.sourceforge.net/lists/listinfo/jewelcli-users</unsubscribe>
      <post>jewelcli-users@lists.sourceforge.net</post>
      <archive />
    </mailingList>
  </mailingLists>
  <reporting>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-javadoc-plugin</artifactId>
        <reportSets>
          <reportSet>
            <id>html</id>
            <configuration>
              <show>public</show>
            </configuration>
            <reports>
              <report>javadoc</report>
            </reports>
          </reportSet>
        </reportSets>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-jxr-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-surefire-report-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>taglist-maven-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-project-info-reports-plugin</artifactId>
        <reportSets>
         <reportSet>
            <reports>
              <report>index</report>
              <report>dependencies</report>
              <report>project-team</report>
              <report>mailing-list</report>
              <report>issue-tracking</report>
              <report>license</report>
              <report>scm</report>
              <report>summary</report>
            </reports>
        </reportSet>
      </reportSets>
     </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>changelog-maven-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>cobertura-maven-plugin</artifactId>
      </plugin>
    </plugins>
  </reporting>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.4</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-surefire-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>cobertura-maven-plugin</artifactId>
        <executions>
          <execution>
            <goals>
              <goal>clean</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-resources-plugin</artifactId>
      </plugin>
    </plugins>
  </build>
</project>
